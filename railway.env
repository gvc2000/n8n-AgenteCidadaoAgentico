# URL da API do MCP (Railway)
MCP_API_URL=https://seu-projeto.up.railway.app

# Chave de encriptação do n8n (gerar uma segura em produção)
N8N_ENCRYPTION_KEY=minha-chave-secreta-muito-segura-123

# Configurações Básicas
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=sua_senha_segura_aqui

# Configuração de Host/Webhook
WEBHOOK_URL=https://seu-app-n8n.up.railway.app

# Segurança e Execução
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=false
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=336
DB_SQLITE_VACUUM_ON_STARTUP=true

# Configurações opcionais
NODE_ENV=production
GENERIC_TIMEZONE=America/Sao_Paulo
TZ=America/Sao_Paulo
